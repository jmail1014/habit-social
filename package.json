{
  "name": "habit-social",
  "version": "1.0.0",
  "description": "A habit tracker built with the MERN stack that connects people by encouraging each other's goals.",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmail1014/habit-social.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jmail1014/habit-social/issues"
  },
  "homepage": "https://github.com/jmail1014/habit-social#readme",
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "apollo-server": "^3.6.7",
    "graphql": "^16.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "concurrently": "^7.1.0"
  }
}
